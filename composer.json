{
  "name": "bdanzer/danzerpress-2.0",
  "version": "master",
  "description": "Starter theme - mix of Sage and Timber theme",
  "type":"wordpress-theme",
  "minimum-stability" : "stable",
  "authors": [
      {
          "name": "Britt Danzer",
          "email": "britt.danzer@gmail.com"
      }
  ],
  "extra": {
  	"installer-paths": {
      "../../plugins/{$name}/": [
        "wpackagist-plugin/*",
        "type:wordpress-plugin"
      ]
  	}
	},
  "require": {
    "wpackagist-plugin/show-current-template" : "*"
  },
  "autoload": {
      "psr-4": {
          "Danzerpress\\Contexts\\": ["inc/contexts"],
          "Danzerpress\\Controllers\\": ["inc/controllers"],
          "Danzerpress\\Chunks\\": ["inc/chunks"],
          "Danzerpress\\Images\\": ["inc/images"],
          "Danzerpress\\Twig\\": ["inc/twig"],
          "Danzerpress\\Rest\\": ["inc/rest"],
          "Danzerpress\\Ajax\\": ["inc/ajax"]
      }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
    	"type": "vcs",
      "url": "git@github.com:bdanzer/danzerpress-2.0.git"
    },
    {
      "type": "git",
      "url": "git@github.com:bdanzer/wp-child-theme.git"
    }
  ]
}