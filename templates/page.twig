{% extends "templates/base.twig" %}

{% block body %}
	<div class="danzerpress-main-content ">
		<div class="danzerpress-two-thirds danzerpress-col-center">
			<div class="danzerpress-shadow-2 danzerpress-white danzerpress-box">
			    {% for post in posts %}
			        <div class="page-header">
			            <h1>{{ post.title }}</h1>
			        </div>

			        {{ post.content }}
			    {% endfor %}
			</div>
		</div>
	</div>
{% endblock %}